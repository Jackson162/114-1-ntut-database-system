{
  "version": "0.2.0",
  "configurations": [
    {
      // "preLaunchTask": "init or sync seaauth_setting.json",
      "name": "Python: API Server Local DB",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--port", "8000", "--host", "0.0.0.0"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        // location of .venv python interpreter
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": false
    }
  ]
}
